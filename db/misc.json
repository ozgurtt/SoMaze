{"_id":"tiles","_rev":"30-733a86a5ceaac7420532c3b872a7ca36","tiles":[{"name":"Blank","desc":"This is a blank tile that makes up the normal part of the map.","file":"blank.png","hidden":false,"cost":{"DOGE":1},"effect":{"hp":0,"status":"none","rearm":true,"blocking":false}},{"name":"Entrance","desc":"This is the starting point for the player on a puzzle.","file":"entrance.png","hidden":false,"cost":{"DOGE":1},"effect":{"hp":0,"status":"none","rearm":false,"blocking":false}},{"name":"Exit","desc":"This is the ending point for the player on a puzzle.","file":"exit.png","hidden":false,"cost":{"DOGE":1},"effect":{"hp":-1000,"status":"none","rearm":false,"blocking":false}},{"name":"Lava","desc":"Very hot. If you touch this, you'll die instantly.","file":"lava.png","hidden":false,"cost":{"DOGE":1},"effect":{"hp":-1000,"status":"none","rearm":true,"blocking":false}},{"name":"Rock","desc":"This rock can't be crossed.","file":"rock.png","hidden":false,"cost":{"DOGE":1},"effect":{"hp":0,"status":"none","rearm":false,"blocking":true}},{"name":"Mine","desc":"A mine stays hidden until the player steps on it, and then it goes BOOM.","file":"mine.png","hidden":true,"cost":{"DOGE":5},"effect":{"hp":-50,"status":"none","rearm":false,"blocking":false}},{"name":"Bomb","desc":"Bombs can be seen by the player and do a little bit less damage than a mine.","file":"bomb.png","hidden":false,"cost":{"DOGE":3},"effect":{"hp":-35,"status":"none","rearm":false,"blocking":false}},{"name":"Spike","desc":"A spike is hidden until the user steps on it, but then it is active for the rest of the game.","file":"spike.png","hidden":true,"cost":{"DOGE":3},"effect":{"hp":-25,"status":"none","rearm":true,"blocking":false}},{"name":"Nail","desc":"A nail is a much smaller spike that doesn't do nearly as much damage. Still wise to leave it alone though.","file":"nail.png","hidden":true,"cost":{"DOGE":2},"effect":{"hp":-10,"status":"none","rearm":true,"blocking":false}},{"name":"Cactus","desc":"A cactus is visible to players and should probably be left alone.","file":"cactus.png","hidden":false,"cost":{"DOGE":2},"effect":{"hp":-5,"status":"none","rearm":true,"blocking":false}},{"name":"Fire Trap","desc":"Fire traps will light you on fire and do damage over time, walking into water will put you out.","file":"fire.png","hidden":true,"cost":{"DOGE":3},"effect":{"hp":-2,"status":"fire","rearm":true,"blocking":false}},{"name":"Water","desc":"Water can put out fires, and can be walked through.","file":"water.png","hidden":false,"cost":{"DOGE":1},"effect":{"hp":0,"status":"wet","rearm":true,"blocking":false}},{"name":"Poison Trap","desc":"Poison traps will do damage over time, and can be cured by a mushroom.","file":"poison.png","hidden":true,"cost":{"DOGE":4},"effect":{"hp":-5,"status":"poison","rearm":false,"blocking":false}},{"name":"Mushroom","desc":"Mushrooms will give a small health boost to the player.","file":"mushroom.png","hidden":false,"cost":{"DOGE":1},"effect":{"hp":10,"status":"cure","rearm":false,"blocking":false}},{"name":"Deadfall Trap","desc":"This heavy weight will fall on a player and hurt them, and then make them bleed.","file":"weight.png","hidden":true,"cost":{"DOGE":5},"effect":{"hp":-10,"status":"bleeding","rearm":false,"blocking":false}},{"name":"Blue Lock","desc":"This lock can't be crossed and can only be unlocked by a blue key","file":"bluelock.png","hidden":false,"cost":{"DOGE":2},"effect":{"hp":0,"status":"none","rearm":false,"blocking":true},"item":{"unblock":"key-blue"}},{"name":"Blue Key","desc":"This key unlocks a blue lock","file":"bluekey.png","hidden":false,"cost":{"DOGE":1},"effect":{"hp":0,"status":"none","rearm":false,"blocking":false},"item":{"equip":"key-blue"}},{"name":"Green Lock","desc":"This lock can't be crossed and can only be unlocked by a green key","file":"greenlock.png","hidden":false,"cost":{"DOGE":2},"effect":{"hp":0,"status":"none","rearm":false,"blocking":true},"item":{"unblock":"key-green"}},{"name":"Green Key","desc":"This key unlocks a green lock","file":"greenkey.png","hidden":false,"cost":{"DOGE":1},"effect":{"hp":0,"status":"none","rearm":false,"blocking":false},"item":{"equip":"key-green"}}],"statuses":{"fire":{"desc":"You're on fire! You should find some water.","effect":-2,"remove":"wet"},"wet":{"desc":"You're soaking wet.","effect":0,"remove":"none"},"poison":{"desc":"You're poisoned! You should find a mushroom.","effect":-5,"remove":"cure"},"cure":{"desc":"You feel better.","effect":0,"remove":"none"},"bleeding":{"desc":"You are bleeding.","effect":-3,"remove":"death"}}}
